{
  "containerDefinitions": [
    {
      "requiresCompatibilities": ["FARGATE"],
      "name": "frontend",
      "image": "%s.dkr.ecr.eu-north-1.amazonaws.com/indokweb-frontend:prod",
      "essential": true,
      "memory": "512",
      "cpu": "100",
      "networkMode": "awsvpc",
      "portMappings": [
        {
          "protocol": "tcp",
          "containerPort": 3000
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "indokweb-frontend-log",
          "awslogs-region": "eu-north-1",
          "awslogs-stream-prefix": "indokweb-frontend"
        }
      }
    }
  ],
  "family": "indokweb-frontend-td",
  "executionRoleArn": "arn:aws:iam::777854691961:role/ecsTaskExecutionRole",
  "memory": "1024",
  "cpu": "256"
}
