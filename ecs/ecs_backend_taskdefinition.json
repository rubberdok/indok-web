{
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "%s.dkr.ecr.eu-north-1.amazonaws.com/indokweb-backend:prod",
      "essential": true,
      "memory": "512",
      "cpu": "100",
      "portMappings": [
        {
          "protocol": "tcp",
          "containerPort": 8000
        }
      ],
      "environment": [
        {
          "name": "DB_HOST",
          "value": "%s"
        },
        {
          "name": "DB_PASSWORD",
          "value": "%s"
        },
        {
          "name": "SECRET_KEY",
          "value": "%s"
        },
        {
          "name": "DATAPORTEN_SECRET",
          "value": "%s"
        },
        {
          "name": "BOOKING_EMAIL_PASSWORD",
          "value": "%s"
        },
        {
          "name": "GOOGLE_DRIVE_API_KEY",
          "value": "%s"
        },
        {
          "name": "DJANGO_SETTINGS_MODULE",
          "value": "%s"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "indokweb-backend-log",
          "awslogs-region": "eu-north-1",
          "awslogs-stream-prefix": "indokweb-backend"
        }
      }
    }
  ],
  "family": "indokweb-backend-td",
  "executionRoleArn": "arn:aws:iam::777854691961:role/ecsTaskExecutionRole",
  "memory": "1024",
  "cpu": "256"
}
